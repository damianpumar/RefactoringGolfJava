public class Title {

	
	private String name;
	private String donorId;
	private int copyCount = 1;

	public Title(String name, String donorId, Member donor) {
		this.name = name;
		this.donorId = donorId;
	}

	protected int getCopyCount() {
		return copyCount;
	}

	protected String getDonor() {
		return donorId;
	}

	protected String getTitleName() {
		return name;
	}

}